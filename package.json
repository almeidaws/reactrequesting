{
  "version": "2.0.0-alpha.9",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/almeidaws/reactrequesting"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-router-dom": ">=5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-requesting",
  "author": "Fernando Lemos",
  "module": "dist/react-requesting.esm.js",
  "size-limit": [
    {
      "path": "dist/react-requesting.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-requesting.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "husky": "^5.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/query-string": "^6.3.0",
    "loadash": "^1.0.0",
    "query-string": "^6.14.0",
    "react-control-hooks": "^3.0.0-alpha.1"
  }
}
